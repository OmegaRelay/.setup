[init] 
    defaultBranch=main
[core]
    editor = vim
    excludesfile = ~/.gitignore_global
[alias]
    st = status
    br = branch
    co = checkout
    cm = commit
    cmm = "commit -m"
    r = reset
    rsh = "reset --hard"
    rst = "restore --staged"
    undo = "reset HEAD~1"
    tree = "log --oneline --graph --decorate --all"
[include]
    path = ~/.gitconfig-local
